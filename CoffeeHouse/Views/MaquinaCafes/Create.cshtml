@model CoffeeHouse.Models.MaquinaCafe

<table class="table table-striped table-hover">
    <!--thead define o cabeçalho da tabela:
        sucess = muda a cor da barra de título--->
    <thead class="success">
        <!--tr é a linha-->
        <tr>
            <!--th é a coluna-->
            <th>
                <!--html displaynamefor pega o nome da clase que eu estou usando através da expressão lambda:-->
                @Html.DisplayFor(model => model.UmCentavo)
                @Html.DisplayFor(model => model.TipoCafe)
            </th>
           
            <th></th>
        </tr>
    </thead>
